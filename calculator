num1=eval(input("Enter first number: "))
num2=eval(input("Enter second number: "))
operator=input("Enter operator(+,*,/,-): ")
if operator=="*":
  calc=num1*num2
elif operator=="+":
  calc=num1+num2
elif operator=="/":
  calc=num1/num2
elif operator=="-":
  calc=num1-num2
print("Your answer is ",calc)
